<Project Sdk="Microsoft.NET.Sdk">

    <PropertyGroup>
        <TargetFramework>netstandard2.1</TargetFramework>
        <GeneratePackageOnBuild>true</GeneratePackageOnBuild>
        <PackageId>EFPostgresEngagement</PackageId>
        <Title>Entity Framework and Postgres engagement</Title>
        <Authors>Hieu Tran</Authors>
        <PackageProjectUrl>https://github.com/hieutran3010/EFPostgresEngagement</PackageProjectUrl>
        <RepositoryUrl>https://github.com/hieutran3010/EFPostgresEngagement</RepositoryUrl>
        <RepositoryType>Github</RepositoryType>
        <PackageTags>Entity Framework, Postgres Net</PackageTags>
        <RootNamespace>EFPostgresEngagement</RootNamespace>
    </PropertyGroup>

    <ItemGroup>
      <PackageReference Include="Microsoft.AspNetCore.Http.Abstractions" Version="2.2.0" />
      <PackageReference Include="Microsoft.EntityFrameworkCore" Version="3.1.3" />
      <PackageReference Include="Microsoft.Extensions.Configuration.Abstractions" Version="3.1.3" />
      <PackageReference Include="Npgsql.EntityFrameworkCore.PostgreSQL" Version="3.1.3" />
    </ItemGroup>

</Project>
