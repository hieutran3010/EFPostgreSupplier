<Project Sdk="Microsoft.NET.Sdk">

    <PropertyGroup>
        <TargetFramework>netstandard2.1</TargetFramework>
        <GeneratePackageOnBuild>true</GeneratePackageOnBuild>
        <PackageId>EFPostgresEngagement</PackageId>
        <Title>EFPostgresEngagement</Title>
        <Authors>Hieu Tran</Authors>
        <PackageProjectUrl>https://github.com/hieutran3010/EFPostgresEngagement</PackageProjectUrl>
        <RepositoryUrl>https://github.com/hieutran3010/EFPostgresEngagement</RepositoryUrl>
        <RepositoryType>Github</RepositoryType>
        <PackageTags>Entity Framework, Postgres Net</PackageTags>
        <RootNamespace>EFPostgresEngagement</RootNamespace>
        <PackageLicense>https://github.com/hieutran3010/EFPostgresEngagement</PackageLicense>
        <Description>This package helps integrating between Entity Framework and PostgreSQL. In other hand, it provides some common interfaces that would be help for CRUD such as IUnitOfWork, IRepository, just do a little code and let's start.</Description>
        <Copyright>Hieu Tran</Copyright>
        <PackageVersion>5.0.0</PackageVersion>
        <AssemblyVersion>5.0.0</AssemblyVersion>
        <FileVersion>5.0.0</FileVersion>
        <NeutralLanguage>en</NeutralLanguage>
        <PackOnBuild>true</PackOnBuild>
        <Owners>Hieu Tran</Owners>
    </PropertyGroup>

    <PropertyGroup Condition=" '$(Configuration)|$(Platform)' == 'Release|AnyCPU' ">
      <DebugType></DebugType>
    </PropertyGroup>
    <ItemGroup>
      <PackageReference Include="Microsoft.AspNetCore.Http.Abstractions" Version="2.2.0" />
      <PackageReference Include="Microsoft.Extensions.Configuration.Abstractions" Version="5.0.0" />
      <PackageReference Include="Npgsql.EntityFrameworkCore.PostgreSQL" Version="5.0.7" />
    </ItemGroup>

</Project>
